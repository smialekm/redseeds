<?xml version="1.0" encoding="UTF-8"?>
<gmftool:ToolRegistry xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <palette
      title="Palette"
      default="//@palette">
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Elements"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Notion">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/model/icons/notion.gif"
            bundle="eu.remics.gmf2"/>
      </tools>
    </tools>
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Connections"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Generalization">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/model/icons/gen.gif"
            bundle="eu.remics.gmf2"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Attribute Relation">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/model/icons/attr.gif"
            bundle="eu.remics.gmf2"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Directed Relation">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/model/icons/dirrel.gif"
            bundle="eu.remics.gmf2"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Indirect Relation">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/model/icons/indrel.gif"
            bundle="eu.remics.gmf2"/>
      </tools>
    </tools>
  </palette>
</gmftool:ToolRegistry>
